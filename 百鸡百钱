#include <stdio.h>
int main()
{
	int a=0;
	int b=0 ;
	int c=0;
	 
	for (a=0;b<=33;b++)
	{
		for(b=0;a<=20;a++)
		{
			for(c=0;c<=300;c=c+3)
			{
				if(a*5+b*3+c/3==100,a+b+c==100)
				{
				printf("公鸡数量是%d\n",a); 
				printf("母鸡数量是%d\n",b); 
			    printf("小鸡数量是%d\n",c) ;
			    }
				
			}
		}

	}
	return 0;
}

	
